    <Showcase showExtra={false} />
    <section class="page-content">
        <h2>404!!</h2>
        <h2><a href='/index.html' aria-label="Go back to CALIREADS"><img src={bookImage} alt=""></a></h2>
        <h2>Page Not Found</h2>
        <p>We have made a lot of changes to our website lately.</p>
        <p>Try one of these:</p>
        <p><a href="https://calireads.org/index#features">Literacy Strategies</a> | <a href="https://calireads.org/literacy-modules">E-Learning Courses</a> | <a href="https://calireads.org/wordbuilder/" target="_blank">Wordbuilder</a></p>
</section>
</Layout>

<style>

    .page-content p{
        text-align: center;
    }
</style>