---
import Detail from './Resource.astro';
import Resource from '../components/Resource.astro';
import rtResource from '../data/rtResources.json';

---

<div class='section-title'>
    <h2>Reciprocal Teaching Module Resources</h2>
</div>
<div class='section-desc'>
<ul class='resource-stack'>
{rtResource.map(resource => (
    <Resource resourceType={resource.resourceType} category={resource.category} title={resource.title}  arial-label={resource.title} url={resource.url}  />
   ))}
</ul>
</div>
